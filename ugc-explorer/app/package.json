  "publishAppID": "63869baa90bb457d55eb1fc5",
  "files": [
    "dist/*",
    "dist/**/*",
    "server.js"
  ],
  "bin": "server.js",
  "scripts": {
    "serve": "./node_modules/.bin/vite",
    "build": "vite build",
    "capacitor": "cap",
    "capacitor:android:generate:keystore": "keytool -genkey -v -keystore my-release-key.keystore -alias ugc-explorer -keyalg RSA -keysize 2048 -validity 1000000 && keytool -importkeystore -srckeystore my-release-key.keystore -destkeystore my-release-key.keystore -deststoretype pkcs12",
    "capacitor:build": "cd android && ./build.sh",
    "preview": "vite preview --port 5050",
    "postpublish": "EVENT_TYPE=ugc-explorer-app REPO=clabroche/monorepo npx @clabroche/common-trigger-workflow",
    "test": ""
  },
  "type": "module",
  "dependencies": {
    "@capacitor/android": "^3.6.0",
    "@capacitor/cli": "^3.6.0",
    "@capacitor/core": "^3.6.0",
    "@clabroche/common-socket-front": "workspace:*",
    "chart.js": "^3.9.1",
    "chartjs-adapter-luxon": "^1.2.0",
    "debounce": "^1.2.1",
    "express": "^4.18.1",
    "luxon": "^3.0.4",
    "vue-chartjs": "^4.1.2"
  },
  "devDependencies": {
