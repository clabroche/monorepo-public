    "bluebird": "^3.7.2",
    "dayjs": "^1.11.1",
    "lodash-es": "^4.17.21",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "tippy.js": "^6.3.7",
    "uuid": "^8.3.2",
    "vee-validate": "^4.6.1",
    "vite": "^2.9.5",
